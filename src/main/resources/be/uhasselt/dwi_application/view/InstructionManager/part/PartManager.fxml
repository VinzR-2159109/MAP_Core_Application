<VBox prefHeight="1000.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.uhasselt.dwi_application.controller.WorkInstruction.Part.PartManagerController">
    <children>
      <HBox alignment="CENTER">
