              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="58.0" prefWidth="189.0" text="Part Manager" textAlignment="JUSTIFY">
                  <font>
                      <Font size="24.0" />
                  </font>
              </Label>
            <Button fx:id="addPart_btn" mnemonicParsing="false" prefHeight="40.0" prefWidth="86.0" text="Add Part">
               <HBox.margin>
