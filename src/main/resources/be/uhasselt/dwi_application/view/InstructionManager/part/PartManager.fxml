               </HBox.margin></Button>
            <Button fx:id="deleteSelectedParts_btn" layoutX="284.0" layoutY="30.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="135.0" text="Delete Selected">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
        <ScrollPane>
            <VBox fx:id="PartContainer_vbox" prefHeight="920.0" prefWidth="376.0">
                <cursor>
                    <Cursor fx:constant="DEFAULT" />
                </cursor>
            </VBox>
        </ScrollPane>
    </children>
</VBox>
